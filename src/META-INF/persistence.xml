<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="co.edu.poli.regequiposPU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>co.edu.poli.regequipos.entidades.Parametros</class>
    <class>co.edu.poli.regequipos.entidades.Equipo</class>
    <class>co.edu.poli.regequipos.entidades.Registro</class>
    <class>co.edu.poli.regequipos.entidades.Marca</class>
    <class>co.edu.poli.regequipos.entidades.Persona</class>
    <class>co.edu.poli.regequipos.entidades.TipoEquipo</class>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/registro_equipos?zeroDateTimeBehavior=convertToNull"/>
      <property name="javax.persistence.jdbc.user" value="adminRegistro"/>
      <property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver"/>
      <property name="javax.persistence.jdbc.password" value="adminRegistro"/>
    </properties>
  </persistence-unit>
</persistence>
